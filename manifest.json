{
  "manifest_version": 2,
  "name": "Mood Aid",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
         "default_popup": "popup.html",
         "default_title": "News credibility"
    },
  "icons": {
    "16": "/png/quote3.png",
    "48": "/png/quote3.png",
    "128": "/png/quote3.png"
  },  
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js; object-src 'self'"
}